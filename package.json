{
  "name": "refile",
  "version": "0.1.0",
  "main": "lib/index.js",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "coverage": "jest --coverage --config jest.config.js",
    "dev": "ts-node src/index.ts",
    "dev:debug": "node --inspect-brk node_modules/.bin/ts-node src/index.ts",
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "rm -rf lib",
    "build": "tsc --project tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./lib",
    "start": "yarn clean && yarn build && node lib/index.js"
  },
  "dependencies": {
    "fastify": "^3.3.0",
    "fastify-multipart": "^3.1.0",
    "ipfs-http-client": "^46.0.1",
    "web-streams-polyfill": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/busboy": "^0.2.3",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "cids": "^1.0.0",
    "eslint": "^7.7.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.0.2"
  }
}
