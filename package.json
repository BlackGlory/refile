{
  "private": true,
  "name": "refile",
  "version": "0.6.0",
  "type": "module",
  "repository": "git@github.com/BlackGlory/refile.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "deduplicate": "yarn-deduplicate",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand --no-cache --config jest.config.js",
    "test:debug": "cross-env NODE_OPTIONS=--experimental-vm-modules node --inspect-brk jest --runInBand",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage --config jest.config.js",
    "dev": "nodemon",
    "dev:http2": "cross-env REFILE_HTTP2=true nodemon",
    "clean": "rimraf lib",
    "build": "tsc --project tsconfig.build.json",
    "bundle": "webpack --stats-error-details",
    "start": "node dist/index.cjs",
    "start:http2": "cross-env REFILE_HTTP2=true node dist/index.cjs",
    "smoke": "cross-env NODE_ENV=test node dist/index.cjs",
    "docker:build": "docker build --tag refile .",
    "docker:test": "docker-compose --file docker-compose.test.yml run --rm test",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare clean lint build test bundle smoke",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/http-status": "^1.0.3",
    "@blackglory/jest-matchers": "^0.5.0",
    "@blackglory/jest-resolver": "^0.3.0",
    "@blackglory/log": "^1.3.3",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/accepts": "^1.3.5",
    "@types/app-root-path": "^1.2.4",
    "@types/better-sqlite3": "^7.6.2",
    "@types/jest": "^29.2.3",
    "@types/jest-json-schema": "^6.1.1",
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "extra-fetch": "^4.0.1",
    "extra-request": "^8.1.5",
    "extra-response": "^0.4.0",
    "husky": "4",
    "jest": "^29.3.1",
    "jest-extended": "^3.2.0",
    "jest-json-schema": "^6.1.0",
    "jest-resolve": "^29.3.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^9.1.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.0.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "4.8",
    "typescript-transform-paths": "^3.4.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "yarn-deduplicate": "^6.0.0"
  },
  "dependencies": {
    "@blackglory/better-sqlite3-migrations": "^0.1.16",
    "@blackglory/errors": "^2.3.0",
    "@blackglory/prelude": "^0.1.8",
    "@fastify/accepts": "^4.1.0",
    "@fastify/bearer-auth": "^9.0.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/multipart": "^7.3.0",
    "better-sqlite3": "^8.0.0",
    "dotenv": "^16.0.3",
    "extra-filesystem": "^0.4.8",
    "extra-generator": "^0.2.21",
    "extra-lazy": "^1.3.1",
    "extra-promise": "^4.4.0",
    "extra-semver": "^0.1.0",
    "extra-tags": "^0.2.6",
    "fastify": "^4.10.2",
    "iterable-operator": "^2.5.0",
    "migration-files": "^0.4.0",
    "return-style": "^1.0.0",
    "split-hash": "^0.2.1",
    "value-getter": "^0.2.4",
    "you-died": "^0.4.2"
  }
}
